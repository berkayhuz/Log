{
  "RabbitMqSettings": {
    "Host": "rabbitmq",
    "Port": 5672,
    "Username": "guest",
    "Password": "guest",
    "VirtualHost": "/",
    "LogQueueName": "log_queue",
    "SslEnabled": false
  },
  "ConnectionStrings": {
    "Redis": "redis-log:6379"
  },
  "BulkLogOptions": {
    "BatchSize": 500,
    "FlushInterval": "00:00:02",
    "MaxDegreeOfParallelism": 1
  },
  "Jwt": {
    "Issuer": "ControlService",
    "Audience": "ControlServiceUsers",
    "PublicKeyPath": "Secrets/public_key.pem"
  },
  "RequestLoggingOptions": {
    "FieldsToMask": [ "password", "token", "access_token", "refresh_token", "secret", "creditCardNumber" ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
